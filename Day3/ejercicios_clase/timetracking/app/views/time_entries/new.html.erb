<h1>Creating entries for <%= @project.name %></h1>

<%= form_for [@project, @time_entry] do |f|%>
  <%= f.label :hours %>
  <%= f.text_field :hours %>
  <%= f.label :minutes %>
  <%= f.text_field :minutes %>
  <%= f.label :comment %>
  <%= f.text_field :comment %>
  <%= f.label :date %>
  <%= f.date_field :date %>
  <%= f.button %>
<% end %>

<%= link_to("Home", root_path) %>
