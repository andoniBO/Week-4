<%= form_for @concert do |f| %>
  <p><%= f.label :artist %>
  <%= f.text_field :artist %></p>
  <p><%= f.label :venue %>
  <%= f.text_field :venue %></p>
  <p><%= f.label :city %>
  <%= f.text_field :city %></p>
  <p><%= f.label :date %>
  <%= f.date_field :date %></p>
  <p><%= f.label :price %>
  <%= f.number_field :price %></p>
  <p><%= f.label :description %>
  <%= f.text_field :description %></p>
  <%= f.button %>
<% end %>
